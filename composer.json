{
    "name": "hyperf/jet",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "rpc-client",
        "jsonrpc"
    ],
    "description": "",
    "autoload": {
        "psr-4": {
            "Hyperf\\Jet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Jet\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "hyperf/rpc": "2.0.*",
        "hyperf/utils": "2.0.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "suggest": {
        "hyperf/load-balancer": "Use load balancer for rpc client."
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    }
}
